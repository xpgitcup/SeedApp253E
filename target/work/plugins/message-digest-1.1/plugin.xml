<plugin name='message-digest' version='1.1' grailsVersion='1.1 &gt; *'>
  <author>Marcel Overdijk</author>
  <authorEmail>marceloverdijk@gmail.com</authorEmail>
  <title>MessageDigest</title>
  <description>This plugin provides message digest codecs to encode strings.</description>
  <documentation>http://grails.org/Message+Digest+Plugin</documentation>
  <resources>
    <resource>HexCodec</resource>
    <resource>MD5Codec</resource>
    <resource>MD5HexCodec</resource>
    <resource>SHA1Codec</resource>
    <resource>SHA1HexCodec</resource>
  </resources>
  <dependencies />
</plugin>